      "view/item/context": [
        {
          "command": "discloud.apps.backup",
          "when": "view == discloud-apps && workspaceFolderCount"
        },
        {
          "command": "discloud.apps.commit",
          "when": "view == discloud-apps && workspaceFolderCount"
        },
        {
          "command": "discloud.apps.copy.id",
