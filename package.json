        },
        {
          "command": "discloud.apps.delete",
          "when": "view == discloud-apps"
        },
        {
          "command": "discloud.apps.import",
          "group": "inline",
          "when": "view == discloud-apps && workspaceFolderCount && viewItem == TreeItem"
