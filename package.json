{
  "name": "discloud",
  "displayName": "Discloud",
  "description": "Somos uma plataforma de nuvem baseada em contêiner que nasceu com a vontade de oferecer hospedagem fácil, rápida, gratuita e de qualidade.",
  "publisher": "discloud",
  "version": "2.22.50",
  "engines": {
    "vscode": "^1.100.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discloud/vscode-discloud.git"
  },
  "icon": "resources/icons/discloudlogo.png",
  "galleryBanner": {
    "color": "#0b0b0b",
    "theme": "dark"
  },
  "sponsor": {
    "url": "https://discloudbot.com/plans"
  },
  "keywords": [
    "discloud",
    "discloud.app",
    "discloudbot",
    "discord",
    "host"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:discloud.config",
    "onLanguage:discloudignore",
    "onStartupFinished",
    "workspaceContains:.discloudignore",
    "workspaceContains:discloud.config"
  ],
  "main": "out/extension.js",
  "l10n": "l10n",
  "contributes": {
    "commands": [
      {
        "command": "discloud.create.config",
