          "when": "view == discloud-apps"
        },
        {
          "command": "discloud.team.backup",
          "when": "view == discloud-teams && workspaceFolderCount && viewItem =~ /backup_app/"
        },
        {
          "command": "discloud.team.commit",
          "when": "view == discloud-teams && workspaceFolderCount && viewItem =~ /commit_app/"
        },
        {
          "command": "discloud.team.copy.id",
          "when": "view == discloud-teams"
        },
        {
          "command": "discloud.team.import",
          "group": "inline",
          "when": "view == discloud-teams && workspaceFolderCount && viewItem =~ /^TreeItem/ && viewItem =~ /backup_app/"
        },
        {
          "command": "discloud.team.logs",
          "when": "view == discloud-teams && viewItem =~ /logs_app/"
        },
        {
          "command": "discloud.team.logs",
          "group": "inline",
          "when": "view == discloud-teams && viewItem =~ /^TreeItem/ && viewItem =~ /logs_app/"
        },
        {
          "command": "discloud.team.ram",
          "when": "view == discloud-teams && viewItem =~ /edit_ram/"
        },
        {
          "command": "discloud.team.restart",
          "when": "view == discloud-teams && viewItem =~ /restart_app/"
        },
        {
          "command": "discloud.team.start",
          "when": "view == discloud-teams && viewItem =~ /start_app/"
        },
        {
          "command": "discloud.team.status",
          "group": "inline",
          "when": "view == discloud-teams && viewItem =~ /^TreeItem/"
        },
        {
          "command": "discloud.team.stop",
          "when": "view == discloud-teams && viewItem =~ /stop_app/"
        },
        {
          "command": "discloud.user.set.locale",
          "when": "view == discloud-user"
        },
        {
          "submenu": "discloud.apps.mods",
          "when": "view == discloud-apps"
        }
      ],
      "view/title": [
        {
          "command": "discloud.apps.refresh",
          "when": "view == discloud-apps",
          "group": "navigation"
        },
        {
          "command": "discloud.apps.sort.online.activate",
          "when": "view == discloud-apps && !config.discloud.app.sort.online"
        },
        {
          "command": "discloud.apps.sort.online.deactivate",
          "when": "view == discloud-apps && config.discloud.app.sort.online"
        },
        {
          "command": "discloud.team.refresh",
          "when": "view == discloud-teams",
          "group": "navigation"
        },
        {
          "command": "discloud.team.sort.online.activate",
          "when": "view == discloud-teams && !config.discloud.team.sort.online"
        },
        {
          "command": "discloud.team.sort.online.deactivate",
          "when": "view == discloud-teams && config.discloud.team.sort.online"
        },
        {
          "command": "discloud.domain.refresh",
          "when": "view == discloud-domains",
          "group": "navigation"
        },
        {
          "command": "discloud.subdomain.refresh",
          "when": "view == discloud-subdomains",
          "group": "navigation"
        },
        {
          "submenu": "discloud.apps.sort.by",
          "when": "view == discloud-apps"
        },
        {
          "submenu": "discloud.team.sort.by",
          "when": "view == discloud-teams"
        }
      ],
      "explorer/context": [
        {
          "submenu": "discloudsub",
          "group": "1_modification"
        }
      ],
      "discloudsub": [
        {
          "command": "discloud.commit",
          "when": "workspaceFolderCount && config.discloud.token"
        },
        {
          "command": "discloud.create.config",
          "when": "workspaceFolderCount"
        }
      ],
      "discloud.apps.mods": [
        {
          "command": "discloud.apps.mods.add"
        },
        {
          "command": "discloud.apps.mods.edit"
        },
        {
          "command": "discloud.apps.mods.rem"
        }
      ],
      "discloud.apps.sort.by": [
        {
          "command": "discloud.apps.sort.by.none",
          "when": "config.discloud.app.sort.by != none"
        },
        {
          "command": "discloud.apps.sort.by.id.asc",
          "when": "config.discloud.app.sort.by != id.asc"
        },
        {
          "command": "discloud.apps.sort.by.id.desc",
          "when": "config.discloud.app.sort.by != id.desc"
        },
        {
          "command": "discloud.apps.sort.by.memory.usage.asc",
          "when": "config.discloud.app.sort.by != memory.usage.asc"
        },
        {
          "command": "discloud.apps.sort.by.memory.usage.desc",
          "when": "config.discloud.app.sort.by != memory.usage.desc"
        },
        {
          "command": "discloud.apps.sort.by.name.asc",
          "when": "config.discloud.app.sort.by != name.asc"
        },
        {
          "command": "discloud.apps.sort.by.name.desc",
          "when": "config.discloud.app.sort.by != name.desc"
        },
        {
          "command": "discloud.apps.sort.by.started.asc",
          "when": "config.discloud.app.sort.by != started.asc"
        },
        {
          "command": "discloud.apps.sort.by.started.desc",
          "when": "config.discloud.app.sort.by != started.desc"
        }
      ],
      "discloud.team.sort.by": [
        {
          "command": "discloud.team.sort.by.none",
          "when": "config.discloud.team.sort.by != none"
        },
        {
          "command": "discloud.team.sort.by.id.asc",
          "when": "config.discloud.team.sort.by != id.asc"
        },
        {
          "command": "discloud.team.sort.by.id.desc",
          "when": "config.discloud.team.sort.by != id.desc"
        },
        {
          "command": "discloud.team.sort.by.memory.usage.asc",
          "when": "config.discloud.team.sort.by != memory.usage.asc"
        },
        {
          "command": "discloud.team.sort.by.memory.usage.desc",
          "when": "config.discloud.team.sort.by != memory.usage.desc"
        },
        {
          "command": "discloud.team.sort.by.name.asc",
          "when": "config.discloud.team.sort.by != name.asc"
        },
        {
          "command": "discloud.team.sort.by.name.desc",
          "when": "config.discloud.team.sort.by != name.desc"
        },
        {
          "command": "discloud.team.sort.by.started.asc",
          "when": "config.discloud.team.sort.by != started.asc"
        },
        {
          "command": "discloud.team.sort.by.started.desc",
          "when": "config.discloud.team.sort.by != started.desc"
        }
      ]
    },
    "submenus": [
      {
        "label": "Discloud",
        "id": "discloudsub"
      },
      {
        "label": "%submenu.manage.mods%",
        "id": "discloud.apps.mods"
      },
      {
        "label": "%submenu.apps.sort.by%",
        "id": "discloud.apps.sort.by"
      },
      {
        "label": "%submenu.team.sort.by%",
        "id": "discloud.team.sort.by"
      }
    ],
    "views": {
      "discloud-activitybar": [
        {
          "type": "tree",
          "id": "discloud-apps",
          "name": "%view.apps.title%",
          "visibility": "visible",
          "when": "config.discloud.token && !discloudTokenUnauthorized"
        },
        {
          "type": "tree",
          "id": "discloud-teams",
          "name": "%view.team.title%",
          "visibility": "collapsed",
          "when": "config.discloud.token && !discloudTokenUnauthorized"
        },
        {
          "type": "tree",
          "id": "discloud-subdomains",
          "name": "%view.subdomain.title%",
          "visibility": "collapsed",
          "when": "config.discloud.token && !discloudTokenUnauthorized"
        },
        {
          "type": "tree",
          "id": "discloud-domains",
          "name": "%view.domain.title%",
          "visibility": "collapsed",
          "when": "config.discloud.token && !discloudTokenUnauthorized"
        },
        {
          "type": "tree",
          "id": "discloud-user",
          "name": "%view.user.title%",
          "visibility": "collapsed"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "discloud-activitybar",
          "title": "Discloud",
          "icon": "resources/icons/discloud_white_icon.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "discloud-apps",
        "contents": "%welcome%",
        "when": "config.discloud.token"
      },
      {
        "view": "discloud-user",
        "contents": "%welcome.token.missing%",
        "when": "!config.discloud.token"
      },
      {
        "view": "discloud-user",
        "contents": "%welcome.token.unauthorized%",
        "when": "config.discloud.token && discloudTokenUnauthorized"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
