    "configuration": [
      {
        "title": "Discloud",
        "properties": {
          "discloud.debug": {
            "type": "boolean",
            "default": false
          },
          "discloud.token": {
            "type": "string",
            "markdownDescription": "%config.token.description%"
          },
          "discloud.users": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "token": {
                  "type": "string"
                }
              }
            },
            "default": []
          },
          "discloud.api.actions.strategy": {
            "type": "string",
            "default": "socket",
            "enum": [
              "rest",
              "socket"
            ],
            "enumItemLabels": [
              "%config.api.actions.strategy.rest.label%",
              "%config.api.actions.strategy.socket.label%"
            ],
            "title": "%config.api.actions.strategy.title%",
            "markdownDescription": "%config.api.actions.strategy.description%"
          },
          "discloud.app.backup.dir": {
            "type": "string",
            "default": "discloud/backup",
            "title": "%config.app.backup.dir.title%",
            "markdownDescription": "%config.app.backup.dir.description%"
          },
          "discloud.app.import.dir": {
            "type": "string",
            "default": "discloud/import",
            "title": "%config.app.import.dir.title%",
            "markdownDescription": "%config.app.import.dir.description%"
          },
          "discloud.app.notification.status": {
            "type": "boolean",
            "default": true,
            "title": "%config.app.notification.status.title%",
            "markdownDescription": "%config.app.notification.status.description%"
          },
          "discloud.app.show.avatar.instead.status": {
            "type": "string",
            "default": "when.online",
            "enum": [
              "never",
              "when.online",
              "always"
            ],
            "enumItemLabels": [
              "%show.avatar.instead.status.never.label%",
              "%show.avatar.instead.status.when.online.label%",
              "%show.avatar.instead.status.always.label%"
            ],
            "title": "%config.app.show.avatar.instead.status.title%",
            "markdownDescription": "%config.app.show.avatar.instead.status.description%"
          },
          "discloud.app.sort.by": {
            "type": "string",
            "default": "none",
            "enum": [
              "none",
              "id.asc",
              "id.desc",
              "memory.usage.asc",
              "memory.usage.desc",
              "name.asc",
              "name.desc",
              "started.asc",
              "started.desc"
            ],
            "enumItemLabels": [
              "%sort.none.label%",
              "%sort.id.asc.label%",
              "%sort.id.desc.label%",
              "%sort.memory.usage.asc.label%",
              "%sort.memory.usage.desc.label%",
              "%sort.name.asc.label%",
              "%sort.name.desc.label%",
              "%sort.started.asc.label%",
              "%sort.started.desc.label%"
            ],
            "title": "%config.app.sort.by.title%",
            "markdownDescription": "%config.app.sort.by.description%"
          },
          "discloud.app.sort.online": {
            "type": "boolean",
            "default": true,
            "title": "%config.app.sort.online.title%",
            "markdownDescription": "%config.app.sort.online.description%"
          },
          "discloud.status.bar.behavior": {
            "type": "string",
            "default": "logs",
            "enum": [
              "commit",
              "logs",
              "upload"
            ],
            "enumItemLabels": [
              "%command.commit%",
              "%command.logs%",
              "%command.upload%"
            ],
            "title": "%config.status.bar.behavior.title%",
            "markdownDescription": "%config.status.bar.behavior.description%"
          },
          "discloud.team.backup.dir": {
            "type": "string",
            "default": "discloud/backup",
            "title": "%config.team.backup.dir.title%",
            "markdownDescription": "%config.team.backup.dir.description%"
          },
          "discloud.team.import.dir": {
            "type": "string",
            "default": "discloud/import",
            "title": "%config.team.import.dir.title%",
            "markdownDescription": "%config.team.import.dir.description%"
          },
          "discloud.team.app.notification.status": {
            "type": "boolean",
            "default": true,
            "title": "%config.team.app.notification.status.title%",
            "markdownDescription": "%config.team.app.notification.status.description%"
          },
          "discloud.team.sort.by": {
            "type": "string",
            "default": "none",
            "enum": [
              "none",
              "id.asc",
              "id.desc",
              "memory.usage.asc",
              "memory.usage.desc",
              "name.asc",
              "name.desc",
              "started.asc",
              "started.desc"
            ],
            "enumItemLabels": [
              "%sort.none.label%",
              "%sort.id.asc.label%",
              "%sort.id.desc.label%",
              "%sort.memory.usage.asc.label%",
              "%sort.memory.usage.desc.label%",
              "%sort.name.asc.label%",
              "%sort.name.desc.label%",
              "%sort.started.asc.label%",
              "%sort.started.desc.label%"
            ],
            "title": "%config.team.sort.by.title%",
            "markdownDescription": "%config.team.sort.by.description%"
          },
          "discloud.team.sort.online": {
            "type": "boolean",
            "default": true,
            "title": "%config.team.sort.online.title%",
            "markdownDescription": "%config.team.sort.online.description%"
          }
        }
      }
    ],
    "grammars": [
      {
        "language": "discloud.config",
        "scopeName": "source.discloud",
        "path": "./syntaxes/dc.tmLanguage.json"
      },
      {
        "language": "discloudignore",
        "scopeName": "text.discloudignore",
        "path": "./syntaxes/di.tmLanguage.json"
      }
