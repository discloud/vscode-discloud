{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "scopeName": "text.discloudignore",
  "patterns": [
    {
      "comment": "Comments",
      "match": "\\s*(#.*)",
      "captures": {
        "1": {
          "patterns": [
            {
              "include": "#inline"
            }
          ]
        }
      }
    }
  ],
  "repository": {
    "inline": {
      "comment": "inline",
      "match": "\\s*(#.*)",
      "name": "comment.line.number-sign",
      "captures": {
        "1": {
          "name": "punctuation.definition.comment"
        }
      }
    }
  }
}
